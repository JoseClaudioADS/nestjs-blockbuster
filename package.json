{
    "name": "curso-blockbuster",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/config/database/orm-config.ts",
        "typeorm:create": "npm run typeorm migration:create -- -n",
        "typeorm:generate": "npm run typeorm migration:generate -- -n",
        "typeorm:revert": "npm run typeorm migration:revert",
        "typeorm:run": "npm run typeorm migration:run"
    },
    "dependencies": {
        "@nestjs/bull": "^0.1.1",
        "@nestjs/common": "^7.0.7",
        "@nestjs/config": "^0.4.0",
        "@nestjs/core": "^7.0.7",
        "@nestjs/jwt": "^7.0.0",
        "@nestjs/passport": "^7.0.0",
        "@nestjs/platform-express": "^7.0.7",
        "@nestjs/platform-socket.io": "^7.0.7",
        "@nestjs/typeorm": "^7.0.0",
        "@nestjs/websockets": "^7.0.7",
        "@sentry/node": "^5.15.4",
        "bcrypt": "^4.0.1",
        "bull": "^3.13.0",
        "cache-manager": "^3.2.1",
        "class-transformer": "^0.2.3",
        "dayjs": "^1.8.24",
        "dotenv": "^8.2.0",
        "nodemailer": "^6.4.6",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "passport-local": "^1.0.0",
        "pg": "^8.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "typeorm": "^0.2.25",
        "yup": "^0.28.3"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.1.2",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.7",
        "@types/bull": "^3.12.2",
        "@types/express": "^4.17.3",
        "@types/jest": "25.1.4",
        "@types/multer": "^1.4.2",
        "@types/node": "^13.9.1",
        "@types/nodemailer": "^6.4.0",
        "@types/passport-jwt": "^3.0.3",
        "@types/socket.io": "^2.1.4",
        "@types/supertest": "^2.0.8",
        "@types/yup": "^0.26.35",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "ts-jest": "25.2.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
